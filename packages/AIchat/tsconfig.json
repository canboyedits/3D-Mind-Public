{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "module": "Node16",
    "rootDir": "src",
    "outDir": "dist",
    "moduleResolution": "node16",
    "moduleDetection": "force",
    "jsx": "react-jsx",
    "noEmit": true,
    "types": ["node"],
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": ["src"]
}
